<%= form_for @grade, url: create_grade_path(@user) do |f| %>
    <div class="form-group">
        <%= f.label 'Ocena' %>
        <%= f.select :result, [1, 2, 3, 4, 5, 6], {}, class: 'form-control' %> 
    </div>
    <div class="form-group">
        <%= f.submit 'Zapisz', class: 'btn btn-success' %>
    </div>
<% end %>
